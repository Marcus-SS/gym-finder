<div class="container">

<div>
<h1><strong>New gym<strong></h1>
</div>

<div>
<%= simple_form_for [@gym] do |f| %>
  <%#= f.input :images, as: :file, input_html: { multiple: true } %>
  <%= f.input :preview, label: "Gym img url" %>
  <%= f.input :name %>
  <%= f.input :city %>
  <%= f.input :address %>
  <%= f.input :lat %>
  <%= f.input :lng %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
</div>
</div>
